# pip install -r requirements.txt
# Create virtual env in .venv folder
python -m venv .venv

source .venv/bin/activate

# create a branch from cs
git checkout -b dev

# push - need only once
git push --set-upstream origin dev 

# Start working in Dev, commit changes
git add .
git commit -m "Add transform test"

# After the first push â€¦and it will automatically push to origin/dev
git push

# merge dev -> main
git checkout main
git merge dev
git push

# untrack


